{
    "version":2,
    "builds":[
        {
            
            "src/main.ts": {
                "memory": 3008,
                "maxDuration": 60
              },
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]

}
